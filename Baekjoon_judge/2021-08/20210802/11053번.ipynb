   ],
   "source": [
    "N = int(input())\n",
    "array = list(map(int, input().split()))\n",
    "dp = [1] * N\n",
    "\n",
    "\n",
    "for i in range(1, len(array)): # 비교 메인\n",
    "    for j in range(i): # 그 이전\n",
    "        if array[i] > array[j]:\n",
    "            dp[i] = max(dp[i], dp[j] + 1)\n",
    "            \n",
    "print(max(dp))"
   ]
  }
